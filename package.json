{
  "name": "angular-tags-input-ws",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "build": "ng build",
    "build:lib": "ng build angular-tags-input",
    "test": "jest",
    "lint": "ng lint",
    "copy:readme": "cp ./README.md ./projects/angular-tags-input/",
